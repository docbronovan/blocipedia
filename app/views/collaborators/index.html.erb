<h1>Collaborators for <%= @wiki.title%></h1>

<%= form_for @wiki do |f| %>
  <%= f.collection_check_boxes(:user_ids, @users, :id, :name, {:item_wrapper_tag => 'checkbox_container'}) do |b| %>
    <div> <%= b.label(:"data-value" => b.value) { b.check_box + b.text } %></div>
   <% end %>
  <div class="form-group">
      <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
<% end %>

